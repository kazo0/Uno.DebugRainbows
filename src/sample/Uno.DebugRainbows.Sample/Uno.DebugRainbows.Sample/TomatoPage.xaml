<Page x:Class="Uno.DebugRainbows.Sample.TomatoPage"
	  xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
	  xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
	  xmlns:local="using:Uno.DebugRainbows.Sample"
	  xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
	  xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
	  mc:Ignorable="d"
	  xmlns:dbg="using:Uno.DebugRainbows"
	  dbg:DebugRainbow.Tomato="True"
	  Background="{ThemeResource ApplicationPageBackgroundThemeBrush}">

	<Grid Padding="20">
		<Grid Padding="20">
			<Grid Padding="20">
				<Grid Padding="20">
					<Grid Padding="20">
						<Grid Padding="20">
							<Grid Padding="20">
								<Border Padding="20">
									<Image Source="ms-appx:///Uno.DebugRainbows.Sample/Assets/tomato.png"
										   VerticalAlignment="Center"
										   HorizontalAlignment="Center" />
								</Border>
							</Grid>
						</Grid>
					</Grid>
				</Grid>
			</Grid>
		</Grid>
	</Grid>
</Page>
